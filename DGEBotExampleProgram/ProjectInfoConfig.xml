<?xml version="1.0" encoding="UTF-8"?>
<!-- This is the project config, Edit if you use a different repository than DGE-DiscordBot, or multiple ones -->
<project>
    <separator>></separator> <!-- The separator character used for the v-latest, dl-latest, and options splitting -->
    <repos>
        <name>DGE-Bot</name>
        <v-latest>github>Nothender>EnderEngine</v-latest> <!-- The repository from which the latest project version is fetched; note the separation is done with '>' -->
        <!-- Type/website > Owner > Repository > release asset index to download (here 0 is app.zip) -->
        <dl-latest>github>Nothender>EnderEngine>0</dl-latest> <!-- The repository from which the latest project version is downloaded -->
        <options>github</options> <!-- Options to create the fetcher, ex : the type/website, login token, etc... -->
        <module>DGE-Framework</module> <!-- Name of the modules used to calculate the current project version -->
        <module>DGE-Bot</module>
        <module>DGE-Engine</module>
    </repos>
</project>